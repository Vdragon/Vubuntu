shown-in-menu = ['UTF-8', 'BIG5', 'BIG5-HKSCS', 'GB18030', 'GB2312', 'GBK', 'GB2312', 'GBK', 'SHIFT_JIS', 'ISO-8859-15', 'UTF-16']
